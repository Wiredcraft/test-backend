{
  "name": "test-backend",
  "version": "1.0.0",
  "description": "Wiredcraft Back-end Developer Test",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "@dyhpoon/egg-passport-auth0": "^1.1.1",
